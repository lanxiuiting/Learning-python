#绘制六角形
import turtle
turtle.setup(900,350,200,200)
turtle.penup()
turtle.fd(-250)
turtle.pendown()
turtle.pensize(25)
turtle.seth(360)
turtle.pencolor("violet")
turtle.fd(60)
turtle.seth(120)
turtle.fd(60)
turtle.seth(240)
turtle.fd(60)
turtle.seth(120)
turtle.fd(40)
turtle.seth(0)
turtle.fd(60)
turtle.seth(210)
turtle.fd(60)
turtle.seth(120)
turtle.fd(20)
